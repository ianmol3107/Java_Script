let dict={
  bodycon: "close-fitting",
  appreciate: "recognize the full width of",
  ataraxia: "a state of freedom from emotional disturbance and anxiety",
  yakka: "Work especially Hard work"
}
console.log(dict.yakka)      // we can use both syntex to print value of dictionary
console.log(dict['bodycon']) // we can use this too
console.log(dict["ataraxia"])  // we can use this too
